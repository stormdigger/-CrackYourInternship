class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        int n=nums.size();
        vector<int>st(n+1,0);
        vector<int>res;
        for(auto it:nums)
        {
            st[it]++;
        }
        for(int i=0;i<=nums.size();i++)
        {
            if(st[i]==2)
            {
                res.push_back(i);
            }
        }
        return res;
    }
};
