class Solution{
  public:
    int n=nums.size();
    vector<long long int> prods(n, 1);
    
    long long int leftProduct = 1, rightProduct = 1;
    
    for (int i = 0; i < n; i++) {
        prods[i] = leftProduct;
        leftProduct *= nums[i];
    }
    
    for (int i = n - 1; i >= 0; i--) {
        prods[i] *= rightProduct;
        rightProduct *= nums[i];
    }
    
    return prods;
};
