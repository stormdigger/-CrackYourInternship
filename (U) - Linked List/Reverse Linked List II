class Solution {
public:
    ListNode* reverseBetween(ListNode* head, int left, int right) {
        ListNode *prev=NULL;
        ListNode *curr=head;
        ListNode *Next;
        while(curr!=NULL)
        {
            Next=curr->next;
            curr->next=prev;
            prev=curr;
            curr=Next;
        }
        ListNode *temp=prev;
        ListNode *temp1=prev;
        while(temp!=NULL)
        {
            temp=temp->next;
        }
        prev->next=temp;
        head=temp1;
        return head;
    }
};
