class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int n=nums.size();
        vector<int>mp(n,0);
        for(int i=0;i<nums.size();i++)
        {
            mp[nums[i]]++;
        }
        for(int i=0;i<mp.size();i++)
        {
            if(mp[i]>1)
            {
                return i;
            }
        }
        return -1;
    }
};
